# MLproject
name: MLops_Final_Project

conda_env: python_env.yaml

entry_points:
  train_lr:
    command: "python train.py --model LR --C {C} --solver {solver} --max_iter {max_iter}"
    parameters:
      C: {type: float, default: 1.0}
      solver: {type: str, default: "liblinear"}
      max_iter: {type: int, default: 100}